@model IEnumerable<ContraloriaNDSWeb.Models.PrincipiosEticos>

@{
    ViewBag.Title = "PrincipiosEticos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="slice animate-hover-slide">
    <div class="w-section inverse blog-grid">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-md-12">
                            @*<div class="w-box blog-post">*@
                                <div class="col-md-12">
                                    <figure>
                                        
                                        <div class="col-md-6">
                                            <h1>Principios Eticos</h1>
                                            @foreach (var item in Model)
                                            {
                                                <blockquote>
                                                    <p style="font-size:medium;color:black;text-align:justify">@Html.DisplayFor(modelItem => item.Descripcion)</p>

                                                </blockquote>
                                            }
                                        </div>
                                        <div class="col-md-6">
                                            <br />
                                            <br /><br />
                                            <img src="~/Content/WebImages/contraloria.png" class="img img-feature" />
                                            
                                        </div>
                                        

                                    </figure>
                                </div>

                            @*</div>*@
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</section>