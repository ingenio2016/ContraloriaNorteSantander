@model ContraloriaNDSWeb.Models.MisionVision

@{
    ViewBag.Title = "Misión y Visión";
}

<h2>Administración Web</h2>

<h3>Está seguro que desea borrar este registro?</h3>
<div>
    <h4>Borrar registro</h4>
    <hr />
    <dl class="dl-horizontal">
       
        <dt>
            @Html.DisplayNameFor(model => model.Mision)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Mision)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Vision)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Vision)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Borrar" class="btn btn-danger" /> |
            @Html.ActionLink("Volver al Listado", "Index", new { }, new { @class = "btn btn-success" })
        </div>
    }
</div>
